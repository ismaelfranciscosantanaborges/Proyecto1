@model User
@{Layout = null; }
<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Perfil</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='~/css/sitio.css' />
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.js"></script>
    <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" />
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />

</head>
<body>
    <div class="container">
        <div class="row registercontentent">

            <div class="col-5  part_img1">
            </div>
            <div class="col-7  formlog1 ">

                <a class="navbar-brand mb-2 mt-3  " style="margin-left:215px;margin-right:260px;">
                    <img src="~/images/logo.png" alt="">
                </a>
                <h5 class="my-4">Create your profesional perfil</h5>
                <form action="">
                    <div class="form-group row">
                        <div class="col-12 col-md-6 mb-3">
                            <input type="text" class="form-control" asp-for="Nombre" placeholder="Name" name="name" id="name">
                        </div>
                        <div class="col-12 col-md-6 mb-3">
                            <input type="text" class="form-control" asp-for="Apellido" placeholder="Lastname" name="lastname" id="lastname">
                        </div>
                        <div class="col-12  mb-1">
                            <input type="email" class="form-control btn-block" asp-for="Correo" placeholder="Email address" name="email" id="email">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-12 col-md-6 mb-3">
                            <input type="text" class="form-control" asp-for="Profesional" placeholder="Profetion" name="profetion" id="profetion">
                        </div>
                        <div class="col-12 col-md-6 mb-3">
                            <input type="text" class="location btn-block py-1" asp-for="Telefono" placeholder="Telephone" name="Telephone" id="location">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-12 col-md-6 mb-3">
                            <input type="file" class="form-control" asp-for="Aptitudes" placeholder="Curriculum " name="Curriculum" id="Curriculum">
                        </div>
                        <div class="col-12 col-md-6 mb-3">
                            <input type="text" class=" py-1 btn-block" asp-for="Cedula" placeholder="Identity Card" name="cedula" id="cedula">
                        </div>
                    </div>
                    <input type="submit" class="btn  btn-block mb-3" value="SEND">
                </form>
               

            </div>
        </div>

    </div>

</body>
</html>
