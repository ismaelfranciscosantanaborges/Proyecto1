@model List<ProyectoWeb.Models.BaseUser>
<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Page Title</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
</head>
<body>
    <h2>Estamos en la Pagina donde se mostrara todos los trabajos disponible para postularse</h2>

    @{
        foreach (var Usuario in Model)
        {
            <div style="margin=30px">
                <div>
                    <b>Id:</b> @Usuario.Id
                </div>
                <div>
                    <b>Nombre:</b> @Usuario.Nombre
                </div>
                <div>
                    <b>Apellido:</b> @Usuario.Apellido
                </div>
                <div>
                    <b>Usuario:</b> @Usuario.UsuarioNombre
                </div>
                <div>
                    <b>Contrase√±a:</b> @Usuario.UsuarioContra
                </div>
                <div>
                    <b>Correo:</b> @Usuario.Correo
                </div>
                <div>
                    <b>Tipo de Usuario:</b> @Usuario.TipoUsuario
                </div>
                <div>
                    <b>Telefono:</b> @Usuario.Telefono
                </div>
                <div>
                    <b>Imagen:</b> @Usuario.Imagen
                </div>
                <div>
                    <b>Aptitudes:</b> @Usuario.Aptitudes
                </div>
                <hr>
            </div>
        }
    }

</body>
</html>